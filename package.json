{
  "name": "echoes",
  "version": "2.0.0",
  "description": "Echoes Media Player - free open source media player based on youtube results",
  "main": "src/index.html",
  "directories": {},
  "scripts": {
    "pioneer": "node_modules/pioneer/bin/pioneer",
    "e2e": "protractor protractor.conf.js",
    "e2ed": "protractor debug protractor.conf.js",
    "e2e:remote": "protractor ./config/config/protractor.conf.bs.js",
    "test": "NODE_ENV='test' karma start",
    "testd": "DEBUG=true npm test",
    "bdd": "NODE_ENV='test' BDD=true npm test",
    "start": "NODE_ENV='development' npm run dev",
    "clean": "npm cache clean && rimraf ./dist ./node_modules",
    "wpw": "webpack -d --watch",
    "dev": "webpack-dev-server --hot --inline --progress --colors --content-base src --port 9001",
    "prod": "rimraf dist/ && NODE_ENV='production' webpack --config=webpack.config.production.js -p",
    "prod:serve": "lite-server -c config/prod-serve.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orizens/echoes.git"
  },
  "keywords": [
    "echoes",
    "echotube",
    "echoes-player",
    "music",
    "media",
    "video"
  ],
  "author": "Oren Farhi",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "angular-local-storage": "0.5.2",
    "angular-mocks": "1.6.4",
    "angular-route": "1.6.4",
    "angular-sanitize": "1.6.4",
    "angular-socialshare": "2.3.8",
    "angular-sortable-view": "0.0.15",
    "angular-toastr": "2.1.1",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-router": "1.0.0",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "3.3.7",
    "browser-sync": "2.18.8",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.1",
    "es6-shim": "0.35.3",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.11.1",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.4",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "jasmine-core": "2.6.1",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.6.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.1.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "2.0.3",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "ng-annotate-loader": "0.6.0",
    "ng-annotate-webpack-plugin": "0.2.0",
    "ng-infinite-scroll": "1.3.0",
    "ngtemplate-loader": "1.3.1",
    "phantomjs-prebuilt": "2.1.14",
    "rimraf": "2.6.1",
    "run-sequence": "1.2.2",
    "selenium-webdriver": "3.4.0",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.0",
    "webpack-dev-server": "2.4.5"
  }
}
